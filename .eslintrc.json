{
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": [2, "always"],
    "dot-notation": [2],
    "no-console": 0
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2021
  },
  "plugins": ["svelte3"],
  "overrides": [
    {
      "files": ["*.svelte"],
      "processor": "svelte3/svelte3"
    }
  ]
}
